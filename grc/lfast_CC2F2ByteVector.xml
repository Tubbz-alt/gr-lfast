<block>
  <name>Accelerated Complex2Real2Byte2Vector</name>
  <key>lfast_CC2F2ByteVector</key>
  <category>[LFAST Accelerated Blocks]</category>
  <import>import lfast</import>
  <make>lfast.CC2F2ByteVector($scale, $vecLength, $num_items)</make>
  <param>
    <name>Scale</name>
    <key>scale</key>
    <value>1</value>
    <type>int</type>
  </param>
  <param>
    <name>Vector length</name>
    <key>vecLength</key>
    <value>1</value>
    <type>int</type>
  </param>
	<param>
		<name>Num Items</name>
		<key>num_items</key>
		<value>2</value>
		<type>int</type>
	</param>
	<check>$num_items &gt; 0</check>
	<check>$vecLength &gt;= 1</check>
  <sink>
    <name>in</name>
    <type>complex</type>
		<vlen>$vecLength</vlen>
  </sink>
	<source>
		<name>out</name>
		<type>byte</type>
		<vlen>$vecLength*$num_items</vlen>
	</source>
</block>
